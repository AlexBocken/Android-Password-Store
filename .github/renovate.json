{
  "extends": [
    "config:base",
    ":dependencyDashboard",
    "helpers:pinGitHubActionDigests",
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "^kotlin",
        "^org.jetbrains.kotlin",
        "^androidx.compose.compiler"
      ],
      "groupName": "kotlin"
    },
    {
      "managers": [
        "gradle"
      ],
      "matchPackagePatterns": [
        "^androidx.appcompat"
      ],
      "enabled": false
    }
  ]
}
